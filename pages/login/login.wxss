
page {
  background: #f5f5f5;
  font-size: 30rpx;
}

.content {
  margin: 0 20rpx;
}

.image {
  margin: 0 auto;
  width: 187rpx;
  height: 199rpx;
}

.form {
  width: 710rpx;
  height: 219rpx;
  border-radius: 15rpx;
  overflow: hidden;
}

input {
  height: 109rpx;
  padding-left: 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #f5f5f5;
 
}

input:last-child {
  border-bottom: 0;
}

button {
  margin-top: 60rpx;
  width: 710rpx;
  height: 82rpx;
  line-height: 82rpx;
  background: #DF5E29;
  border-radius: 41rpx;
  color: #fff;
}
.click {
  margin-top: 80rpx;
  justify-content: center;
}
.mid{
  color: #333333;
  margin: 0 20rpx;
}