page {
  background-color: #F2F2F2;
}

image {
  width: 100%;
  height: 100%;
  vertical-align: top;
  /* vertical-align: middle; */
}

.kong {
  height: 10rpx;
}

.top {
  padding: 0 35rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

  height: 209rpx;
  background: #FEFEFE;
}

.left {
  display: flex;
}

.h-mmg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}

.info {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 35rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #333333;
}

.phone {
  font-size: 26rpx;
  font-weight: 500;
  color: #333333;
}

.right {
  position: relative;
  width: 50rpx;
  height: 50rpx;
}

.next {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 19rpx;
  height: 32rpx;
}

